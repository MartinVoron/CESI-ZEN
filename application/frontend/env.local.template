# Development Environment Variables for CesiZen Frontend
# Copy this to .env.local and modify as needed
# DO NOT COMMIT TO PRODUCTION

# API Configuration
# Use Vite proxy in dev; backend is available at /api
VITE_API_BASE_URL=/api

# Development Mode
NODE_ENV=development

# PWA Configuration (development)
VITE_PWA_ENABLED=true

# Debug Configuration
VITE_DEBUG=true 